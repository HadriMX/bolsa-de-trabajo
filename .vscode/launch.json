{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "name": "Launch firefox",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}\\frontend"
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}\\frontend"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${workspaceFolder}\\frontend\\src\\app\\login\\login.component.ts",
        "outFiles": [
            "${workspaceFolder}/**/*.js"
        ]
    }
    ]
}